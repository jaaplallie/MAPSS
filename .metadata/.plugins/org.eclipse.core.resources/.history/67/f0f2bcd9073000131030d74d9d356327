package Gui;

import java.awt.*;
import javax.swing.*;

public class scenarioSetupWindow extends JPanel {

	/**
	 * 
	 */
	private static final long serialVersionUID = -6622313351868642228L;

	public scenarioSetupWindow() {
		DefaultListModel listModel = new DefaultListModel();
		listModel.addElement("Jane Doe");
		listModel.addElement("John Smith");
		listModel.addElement("Kathy Green");
		listModel.addElement("Jane Smith");
		listModel.addElement("John Green");
		listModel.addElement("Kathy Doe");
		listModel.addElement("Jane Green");
		listModel.addElement("John Doe");
		JList list = new JList(listModel);
		list.setSelectionMode(ListSelectionModel.SINGLE_SELECTION);
		list.setLayoutOrientation(JList.HORIZONTAL_WRAP);
		list.setVisibleRowCount(-1);
		JScrollPane listScroller = new JScrollPane(list);
		listScroller.setPreferredSize(new Dimension(250, 80));
		this.add(list);
	}
	
	

}
